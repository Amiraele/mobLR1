fun main(){
    println("Введите массу 1 предмета")
    var p1:Int = readLine()!!.toInt()
    println("Введите массу 2 предмета")
    var p2:Int = readLine()!!.toInt()
    println("Введите массу 3 предмета")
    var p3:Int = readLine()!!.toInt()
    println("Введите массу 4 предмета")
    var p4:Int = readLine()!!.toInt()
    println("Введите массу 5 предмета")
    var p5:Int = readLine()!!.toInt()

    var sum = p1 + p2 + p3 + p4 + p5
    println(sum)
}
