fun main(){
    println("Введите площадь круга")
    var S1:Double = readLine()!!.toDouble()
    println("Введите площадь квадрата")
    var S2:Double = readLine()!!.toDouble()
    println(S1*2)
    println(S2*3.14)
    if (S1*2 <= 3.14 * S2)
    {
        println("Поместиться")
    }
    else
    {
        println("Не поместиться")
    }
}
